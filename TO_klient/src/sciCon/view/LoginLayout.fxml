<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="loginWindow" prefHeight="250.0" prefWidth="320.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sciCon.controller.LoginRegisterController">
   <children>
      <Button layoutX="110.0" layoutY="200.0" mnemonicParsing="false" onAction="#goToRegistration" onKeyReleased="#goToRegistrationKey" prefWidth="100.0" text="Zarejestruj się">
         <font>
            <Font size="13.0" />
         </font></Button>
      <Label layoutX="85.0" layoutY="170.0" prefWidth="150.0" text="Nie masz jeszcze konta?">
         <font>
            <Font size="14.0" />
         </font></Label>
      <TextField fx:id="loginField" layoutX="50.0" layoutY="25.0" prefHeight="30.0" prefWidth="220.0" promptText="Login">
         <font>
            <Font size="13.0" />
         </font></TextField>
      <PasswordField fx:id="passwordField" layoutX="50.0" layoutY="70.0" minWidth="0.0" prefHeight="30.0" prefWidth="220.0" promptText="Hasło">
         <font>
            <Font size="13.0" />
         </font></PasswordField>
      <Button layoutX="129.0" layoutY="120.0" mnemonicParsing="false" onAction="#loginBtn" onKeyReleased="#loginBtnEnterKey" prefWidth="62.0" text="Zaloguj">
         <font>
            <Font size="13.0" />
         </font></Button>
   </children>
</AnchorPane>
