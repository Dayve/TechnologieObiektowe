<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="registrationWindow" prefHeight="300.0" prefWidth="320.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sciCon.controller.LoginRegisterController">
   <children>
      <Button layoutX="85.0" layoutY="250.0" mnemonicParsing="false" onAction="#goToLogin" onKeyReleased="#cancelBtnEnterKey" text="Anuluj">
         <font>
            <Font size="13.0" />
         </font></Button>
      <TextField fx:id="loginField" layoutX="50.0" layoutY="25.0" prefHeight="30.0" prefWidth="220.0" promptText="Login">
         <font>
            <Font size="13.0" />
         </font></TextField>
      <PasswordField fx:id="passwordField" layoutX="50.0" layoutY="70.0" prefHeight="30.0" prefWidth="220.0" promptText="Hasło">
         <font>
            <Font size="13.0" />
         </font></PasswordField>
      <PasswordField fx:id="passwordRepeatField" layoutX="50.0" layoutY="115.0" prefHeight="30.0" prefWidth="220.0" promptText="Powtórz hasło">
         <font>
            <Font size="13.0" />
         </font></PasswordField>
      <TextField fx:id="nameField" layoutX="50.0" layoutY="160.0" prefHeight="30.0" prefWidth="220.0" promptText="Imię">
         <font>
            <Font size="13.0" />
         </font></TextField>
      <TextField fx:id="surnameField" layoutX="50.0" layoutY="205.0" prefHeight="30.0" prefWidth="220.0" promptText="Nazwisko">
         <font>
            <Font size="13.0" />
         </font></TextField>
      <Button layoutX="168.0" layoutY="250.0" mnemonicParsing="false" onAction="#registerBtn" onKeyReleased="#registerBtnEnterKey" text="Rejestruj">
         <font>
            <Font size="13.0" />
         </font></Button>
   </children>
</AnchorPane>
