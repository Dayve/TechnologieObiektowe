<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="confirmationWindow" prefHeight="180.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sciCon.controller.ConfirmationWindowController">
   <children>
      <ScrollPane hbarPolicy="NEVER" layoutX="25.0" layoutY="25.0" prefHeight="100.0" prefWidth="250.0">
         <content>
            <TextArea fx:id="confirmationMessage" editable="false" maxWidth="248.0" prefHeight="98.0" prefWidth="248.0" wrapText="true">
               <font>
                  <Font size="13.0" />
               </font>
            </TextArea>
         </content>
      </ScrollPane>
      <Button layoutX="166.0" layoutY="139.0" mnemonicParsing="false" onAction="#yesBtn" text="Tak">
         <font>
            <Font size="13.0" />
         </font>
      </Button>
      <Button layoutX="90.0" layoutY="139.0" mnemonicParsing="false" onAction="#noBtn" text="Nie">
         <font>
            <Font size="13.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
