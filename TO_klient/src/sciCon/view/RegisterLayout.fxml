<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="250.0" prefWidth="320.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sciCon.controller.LoginRegisterController">
   <children>
      <Label layoutX="65.0" layoutY="112.0" text="Hasło:" />
      <Label layoutX="65.0" layoutY="72.0" text="Login:" />
      <TextField fx:id="loginField" layoutX="112.0" layoutY="70.0" prefHeight="25.0" prefWidth="177.0" />
      <Button layoutX="188.0" layoutY="189.0" mnemonicParsing="false" onAction="#register" text="Rejestruj" />
      <PasswordField fx:id="passwordField" layoutX="112.0" layoutY="110.0" prefHeight="25.0" prefWidth="177.0" />
      <PasswordField fx:id="passwordRepeatField" layoutX="112.0" layoutY="150.0" prefHeight="25.0" prefWidth="177.0" />
      <Label layoutX="20.0" layoutY="152.0" text="Powtórz hasło:" />
      <Button layoutX="112.0" layoutY="189.0" mnemonicParsing="false" onAction="#goToLogin" text="Anuluj" />
      <Label fx:id="controlLabel" alignment="CENTER" layoutX="65.0" layoutY="15.0" prefHeight="40.0" prefWidth="220.0" textAlignment="JUSTIFY" wrapText="true" />
   </children>
</AnchorPane>
