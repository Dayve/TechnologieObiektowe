<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sciConServer.ServerController">
   <children>
      <AnchorPane layoutX="10.0" layoutY="10.0" prefHeight="200.0" prefWidth="320.0">
         <children>
            <TextArea fx:id="textField" layoutX="20.0" layoutY="25.0" prefHeight="150.0" prefWidth="200.0" />
            <Button layoutX="247.0" layoutY="65.0" mnemonicParsing="false" onAction="#startServer" text="Start" />
            <Button layoutX="247.0" layoutY="126.0" mnemonicParsing="false" onAction="#stopServer" text="Stop" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
